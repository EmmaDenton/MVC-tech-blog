<section class="form-container">
    <div id="signup-form">
        <form  class="form">
                <input type="text" id="username-signup" name="Username" class="user-name" placeholder="Username" required>
                <br>
                <input type="text" id="email-signup" name="email" class="email" placeholder="Email" required>
                <br>
                <input type="password" id="password-signup" name="password" class="password" placeholder="Password" required>
                <br>
            <button type="submit" id="signUpButton" class="active">Create Account</button>
        </form>
        <div class="subtitle">Already Have an Account?</div>
            <button id="returnToLogin">Log in</button>
    </div>
 </section>
 <script src="/js/signup.js"></script>